<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="bot.entity.ClsAnswerEntity" table="cls_answer" schema="bot">
        <id name="id">
            <column name="ID" sql-type="bigint(20)"/>
        </id>
        <property name="isDeleted">
            <column name="IS_DELETED" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="answerText">
            <column name="ANSWER_TEXT" sql-type="text" not-null="true"/>
        </property>
        <property name="answerComment">
            <column name="ANSWER_COMMENT" sql-type="text" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>