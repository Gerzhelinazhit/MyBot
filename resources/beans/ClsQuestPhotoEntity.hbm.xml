<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="bot.entity.ClsQuestPhotoEntity" table="cls_quest_photo" schema="bot">
        <id name="id">
            <column name="ID" sql-type="bigint(20)"/>
        </id>
        <property name="isDeleted">
            <column name="IS_DELETED" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="relFilePath">
            <column name="REL_FILE_PATH" sql-type="blob" not-null="true"/>
        </property>
        <property name="photoText">
            <column name="PHOTO_TEXT" sql-type="blob" not-null="true"/>
        </property>
        <many-to-one name="clsQuestByIdQuest" class="bot.entity.ClsQuestEntity">
            <column name="ID_QUEST"/>
        </many-to-one>
    </class>
</hibernate-mapping>